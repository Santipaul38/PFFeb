<!DOCTYPE html>
<html>
    <%= link_to 'Vehicles', vehicles_path, class: "back-but"%> |
    <%= link_to 'Services', services_path, class: "back-but"%> |
    <%= link_to 'Items', items_path, class: "back-but"%> |
  <head>
    <title>Cars</title>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%= yield %>
  </body>
</html>
